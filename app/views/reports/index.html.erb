<div><%= avatar_for(current_user) %></div>
<%= link_to "Sign out", signout_path %>

<div id="app"></div>

<script type="text/javascript">
  $(function() {
    window.reports = new Reports(<%=raw ReportPresenter.new(@reports).to_json %>);
    window.user = {
      id: <%= current_user.id %>,
      email: <%=raw current_user.email.to_json %>
    };
    Query.run();
  });
</script>
