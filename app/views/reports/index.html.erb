<div id="app"></div>

<script type="text/javascript">
  $(function() {
    window.reports = new Reports(<%=raw ReportPresenter.new(@reports).to_json %>);
    window.user = {
      id: <%= current_user.id %>,
      name: <%=raw current_user.name.to_json %>,
      email: <%=raw current_user.email.to_json %>
    };
    Query.run();
  });
</script>
